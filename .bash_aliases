alias ls='ls -G'
alias ll='ls -lah'
alias be='bundle exec'
alias ghist='git log --pretty=format:"%h %ad | %s%d [%an]" --graph --date=short'
alias spec='~/bin/rspec_wrapper.sh'
alias cuke='~/bin/cuke_wrapper.sh'
alias vi='vim'
alias subl="/Applications/Sublime\ Text\ 2.app/Contents/MacOS/Sublime\ Text\ 2"
alias hist="history | sed -e 's/^[[:space:]]*[0-9]*//g' | sort | uniq -c | sort -n -r | head -5"
